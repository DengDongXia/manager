<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.goods.AddGoodsOrder">

	<insert id="addGoodsOrderByInput" parameterType="domain.goods.GoodsOrder">
		insert into goodsorder(userId,customerName,customerPhone,customerEmail,goodsId,address,price,deliveryId,deliveryCompany,state,time)
		values(#{userId},#{customerName},#{customerPhone},#{customerEmail},#{goodsId},#{address},#{price},#{deliveryId},#{deliveryCompany},#{state},#{time})
	</insert>
	
</mapper>



